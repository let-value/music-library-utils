        "@types/rimraf": "^3.0.2",
        "@types/sqlite3": "^3.1.8",
        "@typescript-eslint/eslint-plugin": "^5.17.0",
        "@typescript-eslint/parser": "^5.17.0",
        "copy": "^0.3.2",
        "copyfiles": "^2.4.1",
        "coveralls": "^3.1.1",
        "cpy-cli": "^4.1.0",
        "eslint": "^8.12.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-editorconfig": "^3.2.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jsdoc": "^38.1.4",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-react": "^7.29.4",
        "eslint-plugin-react-hooks": "^4.4.0",
        "eslint-plugin-unicorn": "^41.0.1",
        "jest": "^27.5.1",
        "pkg": "^5.5.2",
        "prettier": "^2.6.1",
        "rimraf": "^3.0.2",
        "swagger-node-codegen": "^1.6.3",
        "ts-jest": "^27.1.4",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.3"
    },
    "engines": {
        "node": ">=10.0.0"
    },
    "jest": {
        "preset": "ts-jest"
    },
    "pkg": {
        "targets": [
            "win"
        ],
        "outputPath": "build"
    },
    "dependencies": {
        "body-parser": "^1.19.2",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "commander": "^9.1.0",
        "express": "^4.17.3",
        "http": "^0.0.1-security",
        "http-proxy": "^1.18.1",
        "ink": "^3.2.0",
        "ink-link": "^2.0.0",
