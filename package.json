    "devDependencies": {
        "@types/jest": "^23.3.3",
        "@types/node": "^10.11.4",
        "coveralls": "^3.0.2",
        "jest": "^23.6.0",
        "pkg": "^5.5.2",
        "prettier": "^1.14.3",
        "rimraf": "^2.6.2",
        "ts-jest": "^23.10.3",
        "ts-node": "^7.0.1",
        "tslint": "^5.11.0",
        "tslint-config-prettier": "^1.15.0",
        "typescript": "^3.1.1"
    },
    "engines": {
        "node": ">=10.0.0"
    },
    "jest": {
        "preset": "ts-jest"
    },
    "pkg": {
        "targets": [
            "win"
        ],
        "outputPath": "dist"
