    "license": "MIT",
    "repository": "",
    "author": {
        "name": "",
        "email": "",
        "url": ""
    },
    "keywords": [
        ""
    ],
    "files": [
        "dist"
    ],
    "main": "dist/src/index",
    "types": "dist/src/index",
    "bin": "dist/src/index.js",
    "workspaces": [
        "frontend"
    ],
    "scripts": {
        "typeorm": "typeorm-ts-node-esm -d ./src/db/ormconfig.ts",
        "migration:generate": "npm run typeorm migration:generate -p",
        "migration:show": "npm run typeorm migration:show",
        "migration:run": "npm run typeorm migration:run",
        "migration:revert": "npm run typeorm migration:revert",
        "migration:create": "typeorm-ts-node-commonjs migration:create",
        "clean": "rimraf dist && rimraf coverage && rimraf build",
        "lint": "eslint src --ext .ts,.tsx",
        "prepublishOnly": "npm run build",
        "prebuild": "npm run clean && npm run lint && npm run frontend:copy && echo Using TypeScript && tsc --version",
        "frontend:build": "npm run -w frontend build",
        "frontend:copy": "cpy frontend/dist/** dist/src/server/static",
        "build": "tsc --pretty",
        "test": "jest",
        "coverage": "jest --coverage",
        "watch": "npm run build -- --watch",
        "watch:test": "jest --watch",
        "package": "npm run frontend:build && npm run build && pkg ."
    },
    "devDependencies": {
        "@types/body-parser": "^1.19.2",
        "@types/copy": "^0.3.2",
        "@types/copyfiles": "^2.4.1",
